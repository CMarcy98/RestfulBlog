<% include ./partials/header %>

<% blogs.forEach(function(blog) { %>
	<div>
		<h3><%= blog.title %></h3>
		<img src="<%= blog.image %>">
		<span><%= blog.created %></span>
		<p><%= blog.body.substring(0, 100) %>...</p>
		<a href="/blogs/<%= blog._id %>">See More!</a>
	</div>
<% }) %>


<% include ./partials/footer %>
